{
    "name": "angular4-cli-starter",
    "version": "0.0.0",
    "license": "MIT",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "electron-build": "ng build -aot -op .\\electron\\app",
        "electron-copyfiles": "copyfiles -u 1 .\\electron\\main.js .\\electron\\package.json .\\electron\\app && copyfiles .\\src\\res\\* .\\electron\\app",
        "electron-run": "cd .\\electron\\app && electron .",
        "electron-serve": "npm run electron-build && npm run electron-copyfiles && npm run electron-run",
        "cordova-build": "ng build -aot --output-path cordova/www/ --base-href \".\"",
        "cordova-build-prod": "ng build --target=production --environment=prod --output-path cordova/www/ --base-href \".\"",
        "cordova-run-android": "npm run cordova-build && copyfiles .\\src\\res\\* .\\cordova\\www && cd cordova && cordova run android",
        "cordova-run-browser": "npm run cordova-build && copyfiles .\\src\\res\\* .\\cordova\\www && cd cordova && cordova run browser",
        "cordova-serve": "npm run cordova-build && copyfiles .\\src\\res\\* .\\cordova\\www && cd cordova && cordova serve"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^4.4.0-RC.0",
        "@angular/common": "^4.4.0-RC.0",
        "@angular/compiler": "^4.4.0-RC.0",
        "@angular/core": "^4.4.0-RC.0",
        "@angular/forms": "^4.4.0-RC.0",
        "@angular/http": "^4.4.0-RC.0",
        "@angular/platform-browser": "^4.4.0-RC.0",
        "@angular/platform-browser-dynamic": "^4.4.0-RC.0",
        "@angular/router": "^4.4.0-RC.0",
        "@types/core-js": "^0.9.41",
        "angular2-notifications": "^0.7.7",
        "core-js": "^2.4.1",
        "jquery": "^3.2.1",
        "jquery-ui": "^1.12.1",
        "ngx-bootstrap": "^1.6.6",
        "primeng": "^4.2.0-rc.1",
        "rxjs": "^5.1.0",
        "zone.js": "^0.8.4"
    },
    "devDependencies": {
        "@angular/cli": "1.4.1",
        "@angular/compiler-cli": "^4.4.0-RC.0",
        "@types/jasmine": "2.5.54",
        "@types/jquery": "^3.2.12",
        "@types/jqueryui": "^1.11.32",
        "@types/node": "~8.0.28",
        "bootstrap-sass": "^3.3.7",
        "codelyzer": "~3.1.2",
        "copyfiles": "1.2.0",
        "cordova": "~7.0.1",
        "electron": "1.7.6",
        "electron-reload": "^1.1.0",
        "font-awesome": "^4.7.0",
        "jasmine-core": "~2.8.0",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~1.7.1",
        "karma-chrome-launcher": "~2.2.0",
        "karma-cli": "~1.0.1",
        "karma-coverage-istanbul-reporter": "^1.3.0",
        "karma-jasmine": "~1.1.0",
        "karma-jasmine-html-reporter": "^0.2.2",
        "node-sass": "^4.5.2",
        "protractor": "~5.1.0",
        "rcedit": "0.9.0",
        "ts-node": "~3.3.0",
        "tslint": "~5.7.0",
        "typescript": "~2.5.2"
    }
}